/**
 * Main styles
 */

body {
  background-color: #ffffff;
  color: #4F4E4E;
  font: normal 13px/1.5 Verdana, Arial, Helvetica, sans-serif;
}
h1 {
  font-size: 22px;
  font-weight: normal;
  margin: 5px 0;
  color: #047C7D;
  font-family: 'Roboto Slab';
  text-transform: uppercase;
  font-weight: normal !important;
}
h2 {
  font-size: 20px;
  font-weight: normal;
  margin: 5px 0;
  color: #3d5454;
}
h3 {
  font-size: 16px;
  font-weight: normal;
  margin: 5px 0;
  color: #3d5454;
}
h4 {
  font-size: 13px;
  margin: 5px 0;
  color: #3d5454; 
}
h1 a,
h2 a,
h3 a,
h4 a,
h5 a,
h6 a,
h1 a:visited,
h2 a:visited,
h3 a:visited,
h4 a:visited,
h5 a:visited,
h6 a:visited {
  color: #4F4E4E;
  text-decoration: none;
}
h1 a:hover,
h2 a:hover,
h3 a:hover,
h4 a:hover,
h5 a:hover,
h6 a:hover {
  color: #4F4E4E;
  text-decoration: none;
}

strong {
  font-weight: 700;
}

hr {
  border-bottom: 0.4px solid #ECB7B7;
  margin: 1em 0 1em;
}

p {
  font-size: 12px;
  line-height: 2;
  text-align: justify;
  padding: 0 0 1.1em;
}

blockquote {
  background: transparent url(../images/bq-tl.png) no-repeat left top;
  padding-left: 30px;
  margin-left: 10px;
  font-style: italic;
  min-height: 0px;
}
ol{
  list-style-type: decimal;
  margin-left: 25px;
}
#main ul {
  padding: 0 0 1em;
}
#main .region-content ul li {
  margin-left: 3px;
  list-style: none;
  background: url(../images/arrow-red.png) no-repeat left 25%;
  padding: 2px 0 0 1.7em;
  font-size: 12px;
}
ul.links.inline {
  display:block;
  margin-bottom:20px;
  background-color: #F6F6F6;
}
.sidebar ul li {
  margin-left: 0;
  list-style: none;
  background: url(../images/arrow-red.png) no-repeat left;
  padding: 3px 0 0 1.5em;
}
#block-search-form .form-type-textfield #edit-search-block-form--2 {
  width: 200px;
  padding: 3px;
}

.form-submit, .form-button {
  background-color: #FED639;
  border: 0;
  color: #222;
  cursor: pointer;
  overflow: visible;
  padding: 6px 14px;
  margin: 0;
  border-radius: 3px;
}
#block-search-form .form-submit, #block-search-form .form-button {
  background-color: #047C7D;
  border: 0;
  color: #FFF;
  cursor: pointer;
  overflow: visible;
  padding: 6px 9px;
  margin: 0 0 0 -1em;
}

#block-simplenews-1 .simplenews-subscribe label {
  display: none;
}

#forum table {
  border-collapse:collapse;
  width:100%;
}
a {
  color: #047C7D;
  text-decoration: none;
}
a:hover {
  color: #F01919;
  text-decoration: none;
}

/* Page Styles
-------------------------------------------------------------- */

#page-wrapper {
